<header>
  <div class="site-header">
    <div class="site-name">
      <a href="/" title="Return to home page">CSE Motors</a>
    </div>
    <div class="tools">
      <% if(locals.loggedin) { %>
        <a href="/account" title="Go to account management">Welcome <%= locals.accountData.account_firstname %></a>
        <a href="/account/logout" title="Click to log out">Logout</a>
      <% } else { %>
        <a href="/account/login" title="Click to log in">My Account</a>
      <% } %>
    </div>
  </div>
</header>